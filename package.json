{
  "name": "locstique",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:init": "bun ./app/database/init.server.ts",
    "db:seed": "bun ./app/database/seed.server.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "app:run": "pm2 start \"pnpm dev\" --name locstique",
    "app:restart": "pm2 restart locstique",
    "app:logs": "pm2 logs locstique"
  },
  "dependencies": {
    "@ant-design/charts": "^2.3.0",
    "@mjackson/file-storage": "^0.6.1",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "argon2": "^0.43.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "isbot": "^5.1.27",
    "nodemailer": "^7.0.3",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-quill-new": "^3.4.6",
    "react-router": "^7.5.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.76.1",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "antd": "^5.25.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.1",
    "react-number-format": "^5.4.4",
    "react-responsive": "^10.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.4"
